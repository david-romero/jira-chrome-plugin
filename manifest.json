{
	"manifest_version": 2,
	"name": "JIRA Custom",
	"author": "Alejandro M. Pinto Rodriguez - David Romero Alcaide",
	"version": "1.0.0",
	"description": "JIRA App for Time Trackign",
	"app": {
		"background": {
			"scripts": ["background.js"]
		},
		"isDebug": true
	},
	"icons": {
		"16": "icons/time32.png",
        "48": "icons/time64.png",
        "128": "icons/time128.png" 
	},
	"permissions": [
		"unlimitedStorage",
		"notifications",
		"storage",
		"http://*/*",
		"https://*/*"
	]
}